{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": {
      "node": {}
    }
  },
  "rules": {
    "consistent-return": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "global-require": "off",
    "class-methods-use-this": "off"
  }
}
